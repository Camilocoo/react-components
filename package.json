{
  "name": "@breathecode/ui-components",
  "version": "1.0.3",
  "description": "Collection of usefull components for react.js applications around the BreatheCode ecosystem",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode development",
    "storybook": "start-storybook -p 8081",
    "build-storybook": "build-storybook",
    "deploy": "build-storybook && node deploy-github.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/breatheco-de/react-components.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "keywords": [
    "React",
    "BreatheCode"
  ],
  "author": "Alejandro Sanchez",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/breatheco-de/react-components/issues"
  },
  "homepage": "https://github.com/breatheco-de/react-components#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-info": "^4.0.7",
    "@storybook/addon-knobs": "^4.0.7",
    "@storybook/addon-links": "^4.0.7",
    "@storybook/addon-notes": "^4.0.7",
    "@storybook/addons": "^4.0.7",
    "@storybook/react": "^4.0.7",
    "babel-loader": "^8.0.0-beta.6",
    "bc-console": "0.0.2",
    "css-loader": "^0.28.11",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "bootstrap": "^4.1.1",
    "flux": "^3.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.3.1",
    "validator": "^10.9.0"
  }
